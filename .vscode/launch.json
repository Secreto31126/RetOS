{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "gdb",
            "request": "attach",
            "name": "Attach to QEMU",
            "target": ":1234",
            "pathSubstitutions": {
                "/root": "${workspaceRoot}"
            },
            "remote": true,
            "cwd": "${workspaceRoot}",
            "valuesFormatting": "parseText",
            "printCalls": true,
            "autorun": [
                "add-symbol-file /tmp/retos/kernel.elf 0x00100000",
                "add-symbol-file /tmp/retos/0000-sampleCodeModule.elf 0x00400000",
                "add-symbol-file /tmp/retos/0002-tomyland.elf 0x00600000",
                "add-symbol-file /tmp/retos/0003-cat.elf 0x00700000",
                "add-symbol-file /tmp/retos/0004-wc.elf 0x00800000",
                "add-symbol-file /tmp/retos/0005-filter.elf 0x00900000"
            ],
            "stopAtConnect": true
        }
    ]
}