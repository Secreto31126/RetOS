{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "gdb",
            "request": "attach",
            "name": "Attach to QEMU",
            "preLaunchTask": "Launch in debug mode",
            "target": ":1234",
            "pathSubstitutions": {
                "/root": "${workspaceRoot}"
            },
            "remote": true,
            "cwd": "${workspaceRoot}",
            "valuesFormatting": "parseText",
            "printCalls": true,
            "autorun": [
                "add-symbol-file /tmp/retos/kernel.elf 0x00100000",
                "add-symbol-file /tmp/retos/0000-sampleCodeModule.elf 0x00400000",
                "add-symbol-file /tmp/retos/0002-init.elf 0x00600000",
                "add-symbol-file /tmp/retos/0003-cat.elf 0x00605000",
                "add-symbol-file /tmp/retos/0004-wc.elf 0x00610000",
                "add-symbol-file /tmp/retos/0005-filter.elf 0x00615000",
                "add-symbol-file /tmp/retos/0006-loop.elf 0x00620000",
                "add-symbol-file /tmp/retos/0007-grep.elf 0x00625000",
                "add-symbol-file /tmp/retos/0008-sing.elf 0x00630000",
                "add-symbol-file /tmp/retos/0009-less.elf 0x00635000",
                "add-symbol-file /tmp/retos/000a-phylos.elf 0x00640000",
                "add-symbol-file /tmp/retos/000b-tests.elf 0x00645000"
            ],
            "stopAtConnect": true
        }
    ]
}